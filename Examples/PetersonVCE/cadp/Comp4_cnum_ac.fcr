const Max_Proxy:nat is 3
type IntType is 0..0
type BoolType is 0..1
type NatType is nat
type IntInterval is 1..4
process Comp4_cnum_ac
[Call_set_cnum: in IntInterval,
Call_get_cnum: none,
R_get_cnum: out IntInterval]
is
states s0,s1
var value:IntInterval, cnum:IntInterval := 4
from s0
	select 
	Call_set_cnum?value; cnum:=value; to s0
	[]
	Call_get_cnum; to s1
	end
from s1
	R_get_cnum!cnum; to s0

Comp4_cnum_ac