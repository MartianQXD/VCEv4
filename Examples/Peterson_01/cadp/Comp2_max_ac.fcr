const Max_Proxy:nat is 3
type IntType is 0..0
type BoolType is 0..1
type NatType is nat
type IntInterval is 1..4
process Comp2_max_ac
[Call_set_max: in IntInterval,
Call_get_max: none,
R_get_max: out IntInterval]
is
states s0,s1
var value:IntInterval, max:IntInterval := 2
from s0
	select 
	Call_set_max?value; max:=value; to s0
	[]
	Call_get_max; to s1
	end
from s1
	R_get_max!max; to s0

Comp2_max_ac